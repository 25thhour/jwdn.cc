  "bugs": {
    "url": "https://github.com/25thhour/jwdn.cc/issues"
  },
  "homepage": "https://github.com/25thhour/jwdn.cc#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0-beta.9",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.9"
  }
}
